{
  "theme": "One Dark",
  "show_inline_completions": true,
  "vim_mode": true,
  "tab_size": 2,
  "ui_font_size": 14.2,
  "buffer_font_size": 12,
  "ui_font_family": "Zed Plex Sans",
  "buffer_font_family": "Menlo",
  "format_on_save": "on",
  "cursor_shape": "underscore",
  "command_aliases": { "Q": "q", "W": "w" },
  "relative_line_numbers": true,
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "code_actions_on_format": {
    "source.fixAll.eslint": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "inlay_hints": {
    "enabled": false,
    "show_other_hints": false,
    "show_parameter_hints": false,
    "show_type_hints": false
  },
  "tabs": {
    "file_icons": true
  },
  "experimental.theme_overrides": {
    "background": "#363f4a",
    "title_bar.background": "#363f4a",
    "status_bar.background": "#363f4a",
    "editor.background": "#00000000",
    "editor.gutter.background": "#00000000",
    "editor.active_line.background": "#00000000",
    "panel.background": "#00000000",
    "terminal.background": "#00000000",
    "toolbar.background": "#00000003",
    "scrollbar.track.border": "#00000000",
    "scrollbar.track.background": "#00000000",
    "scrollbar.thumb.background": "#ffffff21",
    "tab_bar.background": "#00000000",
    "tab.active_background": "#00000000",
    "tab.inactive_background": "#00000000",
    "elevated_surface.background": "#333b46"
  }
}
